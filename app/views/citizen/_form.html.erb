<h3 class="title "> Cadastro de MunÃ­cipe </h3>
<%= form_for @citien do |f| %>
  <div class="container">
    <div class="row">
      <div class="col">
        <%= f.label :name, class: "control-label" %>
        <%= f.text_field :name, class: "form-control" %>
      </div>
      <div class="col">
        <%= f.label :phone, class: "control-label" %>
        <%= f.text_field :phone, class: "form-control" %>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <%= f.label :email, class: "control-label" %>
        <%= f.text_field :email, class: "form-control" %>
      </div>
      <div class="col">
        <%= f.label :birthdate, class: "control-label" %>
        <%= f.date_field :birthdate, class: "form-control" %>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <%= f.label :cpf, class: "control-label" %>
        <%= f.text_field :cpf, class: "form-control" %>
      </div>
      <div class="col">
        <%= f.label :cns, class: "control-label" %>
        <%= f.text_field :cns, class: "form-control" %>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <%= f.label :photo, class: "control-label" %>
        <%= f.file_field :photo, class: "form-control" %>
      </div>
      <div class="col">
        <%= f.label :status, class: "control-label" %>
        <%= f.boolean :status %>
      </div>
    </div>
    <div class="nested-fields">
      <%= f.fields_for :endereco do |adress_form| %>
        <%= render "adress/form", f: adress_form %>
      <% end %>
    </div>
    <div class="col">
      <%= f.submit 'salvar', class: "btn btn-default btn-primary" %>
      <%= f.submit 'voltar', class: "btn btn-default btn-danger" %>

    </div>
  </div>
<% end %>
